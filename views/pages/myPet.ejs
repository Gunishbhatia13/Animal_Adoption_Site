<% layout('../layouts/base') %>
<style>
  .form_class button {
		color: white;
		margin-left: 250px;
		height: 40px;
		width: 100px;
		border: none;
		border-radius: 15px;
		background-color: #FD9789;
    margin-top: 20px;
	}
</style>
<main>
<div class="support">
  <div class="dog_class">
    <div class="dog">
      <div class="ears"></div>
      
      <div class="body">
        <div class="eyes"></div>
        <div class="beard">
          <div class="mouth">
            <div class="tongue"></div>
          </div>
        </div>
        <div class="belt">
          <div class="locket"></div>
          <div class="dot dot1"></div>
          <div class="dot dot2"></div>
          <div class="dot dot3"></div>
          <div class="dot dot4"></div>
        </div>
        <div class="stomach">
        </div>
        <div class="legs">
          <div class="left"></div>
          <div class="right"></div>
        </div>
      </div>
      <div class="tail">
      </div>
    </div>
  </div>
  <div class="form_class">
    <form action="/support" method="post">
      <input type="text" placeholder="Full Name" name="name" id="">
      <br>
      <input type="text" placeholder="Contact" name="contact">
      <br>
      <input type="text" placeholder="email" name="email" id="">
      <br>
      <textarea name="query" placeholder="Write your query" id="" cols="50" rows="5"></textarea>
      <br>
      <button type="submit">Submit</button>
    </form>
  </div>
</div>
</main>